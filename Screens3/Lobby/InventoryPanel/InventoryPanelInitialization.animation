animation InventoryInitialization
{
  if (initializationAnimation != eInventoryPanelAnimation.kNoAnimation)
  {
    Event("SHOW_INVENTORY_PANEL", arg1=true, arg2=initializationAnimation, arg3=0.2);
  }
  else
  {
      Opacity(".", opacity=1.0);
      Scale("./HangarBackground", x=1.0 ,y=1.0);
  }
  Event("INVENTORY_INITIALIZED");
}