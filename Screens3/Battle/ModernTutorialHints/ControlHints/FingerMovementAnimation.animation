animation FingerMovement
{
    Opacity(".", opacity=0.0, time=0.0, interpolation=EASE_IN_EASE_OUT);
	repeat
	{
		Opacity(".", opacity=1.0, time=0.2, interpolation=EASE_IN_EASE_OUT);
		MoveRel(".", 0, -64.0, 0.75, EASE_IN_EASE_OUT);	
		Opacity(".", opacity=0.0, time=0.2, interpolation=EASE_IN_EASE_OUT);
		MoveRel(".", 0, 64.0, 0, EASE_IN_EASE_OUT);
	}
}